{
	"env": {
		"browser": true,
		"es2021": true,
		"node": true
	},
	"extends": ["plugin:react/recommended", "airbnb", "prettier"],
	"overrides": [],
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["react"],
	"rules": {
		// starter code errors
		"import/no-unresolved": "off",
		"consistent-return": "off",
		"no-underscore-dangle": "off",

		// starter code warns
		"no-console": "off",
		"func-names": "off"
	}
}
